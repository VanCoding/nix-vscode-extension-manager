{
  "name": "nix-vscode-extension-manager",
  "version": "0.0.0",
  "author": "Patrik Stutz @VanCoding",
  "bin": {
    "nvem": "./bin/nvem"
  },
  "bugs": "https://github.com/VanCoding/nix-vscode-extension-manager/issues",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.8",
    "axios": "^0.21.1",
    "commander": "^7.1.0",
    "fs-extra": "^9.1.0",
    "jszip": "^3.6.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@types/node": "^10.17.55",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/VanCoding/nix-vscode-extension-manager",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nvem",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "VanCoding/nix-vscode-extension-manager",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "build":"tsc"
  },
  "types": "lib/index.d.ts"
}
